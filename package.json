{
  "name": "tminase.dev",
  "private": true,
  "description": "blog built with Next.js",
  "version": "2.0.0",
  "author": "tminasen",
  "scripts": {
    "build": "next build && next export",
    "dev": "next",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "next start",
    "create:md": "./scripts/article-scripts/target/release/article-scripts",
    "rename:files": "./scripts/target/release/article-scripts --rename ",
    "generate:rss": "ts-node --project ./scripts/tsconfig.scripts.json ./scripts/generate-rss/GenerateRss.ts",
    "generate:ogp": "ts-node --project ./scripts/tsconfig.scripts.json ./scripts/generate-ogp/GenerateOgp.ts",
    "type-check": "tsc"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.1",
    "next": "^v12.1.0",
    "prism-react-renderer": "^1.3.3",
    "prismjs": "^1.28.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "rehype": "^12.0.1",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "styled-components": "^5.3.5",
    "styled-media-query": "^2.1.2",
    "typescript": "4.6",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "feed": "^4.2.2",
    "prettier": "2.6.2",
    "puppeteer": "^14.1.1",
    "raw-loader": "^4.0.2",
    "ts-node": "^10.8.0"
  },
  "license": "MIT"
}
